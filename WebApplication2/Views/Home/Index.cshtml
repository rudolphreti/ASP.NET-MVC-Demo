@using WebApplication2.Data;
@model List<Person>

<h1>Personen</h1>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Alter</th>
            <!-- Weitere Spalten nach Bedarf -->
        </tr>
    </thead>
    <tbody>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.Name</td>
                <td>@person.Id</td>
                <!-- Weitere Felder nach Bedarf -->
            </tr>
        }
    </tbody>
</table>
